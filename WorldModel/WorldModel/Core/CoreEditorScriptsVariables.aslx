  <editor>
    <appliesto>(function)DecreaseObjectCounter</appliesto>
    <display>Decrease #0 counter #1</display>
    <category>Variables</category>
    <add>Decrease object counter</add>
    <create>DecreaseObjectCounter (,"")</create>
    <onlydisplayif>GetString(game, "_editorstyle") = null</onlydisplayif>

